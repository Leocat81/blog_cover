{
  "scripts": {
    "start": "webpack-dev-server ",
    "scss": "node-sass --output-style compressed -o docs/css src/scss",
    "autoprefix": "postcss docs/css/style.css --use autoprefixer --replace",
    "inline": "inliner --skip-absolute-urls src/index.html > docs/index.html",
    "build": "webpack",
    "lint": "eslint ./src --ext js --fix",
    "pretty": "prettier './src/**/*.{js,scss}' --write"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "webfontloader": "^1.6.26",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "autoprefixer": "^8.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.16.0",
    "css-loader": "^6.3.0",
    "eslint": "^4.19.0",
    "html-webpack-plugin": "^4.5.2",
    "inliner": "^1.9.5",
    "node-sass": "^4.7.2",
    "postcss-cli": "^5.0.0",
    "prettier": "^1.11.1",
    "sass": "^1.32.6",
    "sass-loader": "^12.1.0",
    "style-loader": "^1.3.0",
    "webpack": "^5.53.0",
    "webpack-cli": "^3.1.1"
  }
}
